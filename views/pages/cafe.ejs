<div class="tour_hero " data-rate="-0.06">
    <h1 class="tour_hero__title">
        <%= post.title%> <br />
        <p class="tour_hero__title--sub"><%= post.small_description %></p>
    </h1>
    <ul class="tour_hero__showcase-images">
        <% for(let i = 0; i < post.showcaseImagesUrls.length; i++){ %>
        <li
            class="tour_hero__showcase-images__image"
            style='background-image: url("/images/<%= post.showcaseImagesUrls[i] %>" )'
        ></li>
        <% } %>
    </ul>
</div>

